{
  "hosting": {
    "public": "dist/empty",
    "rewrites": [
      {
        "source": "/foo",
        "destination": "/test.html"
      },
      {
        "source": "/bar",
        "function": "api"
      },
      {
        "source": "**",
        "function": "api"
      }
    ]
  },
  "functions": {
    "source": ".",
    "ignore": [
      "node_modules",
      "scripts",
      "src",
      ".env_sample",
      ".env",
      ".gitignore",
      "tsconfig.json"
    ]
  }
}
